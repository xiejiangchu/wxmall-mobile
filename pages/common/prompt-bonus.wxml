<template name="prompt-bonus">
  <view class="prompt-container" hidden="{{ prompt.hidden }}">
    <view class="prompt-message">
      <image src="{{ prompt.icon || '/assets/images/iconfont-empty.png' }}" class="prompt-icon" />
      <view class="prompt-title">{{ prompt.title || '空空如也' }}</view>
      <view class="prompt-text">{{ prompt.text || '暂时没有优惠券' }}</view>
    </view>
  </view>
</template>