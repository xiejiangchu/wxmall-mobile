page {
  background: #f5f5f5;
  height: 100%;
}

.container {
  position: relative;
  width: 100%;
  min-height: 100%;
  background-color: #fff;
  color: #939393;
}

.scroll-view_H {
  display: flex;
  flex-wrap: nowrap;
  white-space: nowrap;
  align-content: center;
  justify-content: center;
  border-bottom: 1px solid #479de6;
}

.scroll-view_H .scroll-view-item_H {
  display: inline-block;
  padding: 8px;
  color: #999;
  background: #ffffff;
}

.scroll-view_H .scroll-view-item_H.active {
  color: #479de6;
}

.nav_left {
  display: inline-block;
  width: 25%;
  height: 100%;
  background: #f2f2f2;
  text-align: center;
}

.nav_left .nav_left_items {
  height: 30px;
  line-height: 30px;
  padding: 6px 0;
  border-bottom: 1px solid #dedede;
  color: #444;
  font-size: 14px;
  text-overflow: ellipsis;
}

.nav_left .nav_left_items.active {
  background: #fff;
}

.nav_right {
  position: absolute;
  top: 0;
  right: 0;
  flex: 1;
  width: 75%;
  height: 100%;
  padding: 10px;
  background: #fff;
  box-sizing: border-box;
  border-left: 1px solid #479de6;
}

.nav_right .nav_right_items {
  float: left;
  width: 33.33%;
  height: 80px;
  text-align: center;
}

.nav_right .nav_right_items image {
  width: 50px;
  height: 30px;
}

.nav_right .nav_right_items text {
  display: block;
  margin-top: 5px;
  font-size: 10px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: #222;
}

.empty-container {
  margin-top: 20%;
}
